{
	"compilerOptions": {
		"outDir": "dist",
		"incremental": true,
		"strict": true,
		"target": "es2023",
		"module": "es2020",
		"experimentalDecorators": true,
		"sourceMap": false,
		"moduleResolution": "Node",
		"removeComments": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"composite": true,
		"skipLibCheck": true
	},
	"include": ["**/*.ts"]
}
